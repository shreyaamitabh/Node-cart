<body>
<div class="form">
    <h1 style="text-align:center; font-family: serif;">Sign into your account </h1>
  
<form action="/home/user"  method="post">
    <div class="form-group">
      <label for="email" id="textemail">Email:</label>
      <input type="email" class="form-control" id="email" placeholder="Enter email" onkeyup="getemail(this.value)" required name="Email">
      <p id="eValid"></p>
    </div>
    <div class="form-group">
      <label for="pwd" id="textpwd">Password:</label>
      <input type="password" class="form-control" id="pwd" placeholder="Enter password" name="Password">
    </div>
    <div class="form-group form-check">
      <label class="form-check-label">
        <input class="form-check-input" type="checkbox" id="rem" name="remember"> <p id= "remme">Remember me</p>
      </label>
    </div>
    <button type="submit" id="submit" class="btn btn-primary">Submit</button>
  </form> 
  </div>
  

<script src="../js/register.js">
</script>
</body>
